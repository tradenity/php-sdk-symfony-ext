{
  "name": "tradenity/symfony-ext",
  "version":"0.5.0",
  "license": "Apache-2.0",
  "description": "Extension library to ease integration between Tradenity ecommerce PHP SDK and Symphony framework.",
  "authors": [
    {
      "name": "Tradenity Inc.",
      "email": "tradenity@tradenity.com",
      "homepage": "http://www.tradenity.com",
      "role": "Vendor"
    }
  ],
  "support": {
    "email": "support@tradenity.com",
    "issues": "http://github.com/tradenity/php-sdk-symfony-ext/issues",
    "source": "http://github.com/tradenity/php-sdk-symfony-ext"
  },
  "minimum-stability": "beta",
  "require": {
    "php": ">=5.5.0",
    "tradenity/php-sdk": ">=1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "phpdocumentor/phpdocumentor": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Tradenity\\SDK\\": "src/tradenity/sdk"
    }
  },
  "autoload-dev" : {
    "psr-4": {
      "Tradenity\\SDK\\Tests\\" : "tests/tradenity"
    }
  }
}